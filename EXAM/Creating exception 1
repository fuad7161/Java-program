package com.company;

public class Test {
    public static void validate(int age){
        if(age < 18){
            throw new ArithmeticException("not eligible.");
        }else{
            System.out.println("eligible.");
        }
    }
    public static void main(String[] args) {
        validate(17);
        System.out.println("end of code.");
    }
}
